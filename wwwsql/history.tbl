CREATE TABLE HISTORY (
	HISTORYKEY INTEGER NOT NULL PRIMARY KEY AUTO_INCREMENT,
	SECTION VARCHAR(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci NOT NULL,
	CHANGETIME TIMESTAMP NOT NULL,
	CHANGEUSER VARCHAR(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci,
	JSONDATA TEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci
);
CREATE INDEX HISTORY_IDX1 ON HISTORY(SECTION, CHANGETIME);
