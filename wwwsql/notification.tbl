CREATE TABLE NOTIFICATION (
	NOTIFICATIONKEY INTEGER NOT NULL PRIMARY KEY AUTO_INCREMENT,
	MESSAGE VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
	CHANNEL VARCHAR(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
	SUBUSER VARCHAR(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
	USERAVATAR VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
	EVENTTIME TIMESTAMP NULL
);
CREATE INDEX NOTIFICATION_IDX1 ON NOTIFICATION(EVENTTIME DESC);
